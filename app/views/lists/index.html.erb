<div class="container">
  <div class="row cards">
<h1>All lists</h1>
  <h1><span id="banner-typed-text"></span></h1>
    <h1 class="banner-arrow"></h1>
</div>
    <ul>
      <%= @lists.each do |list| %>
        <li><%= link_to list.name, list_path(list)%></li>
        <% if list.photo %>
          <li><%= cl_image_tag list.photo.key , height: 300, width: 400, crop: :fill %></li>
        <% else %>
        <%= "use upload feature" %>
        <% end %>
      <% end %>
    <%=  link_to "add", new_list_path, class: "btn btn-warning"%>
    </ul>
  </div>
</div>
